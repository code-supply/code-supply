export ACCESS_KEY_ID=
export BUCKET_NAME=
export ID_SALT=
export LIVE_VIEW_SIGNING_SALT=
export MIX_ENV=prod
export SECRET_KEY_BASE=
export SENDGRID_API_KEY=

mix ecto.migrate
